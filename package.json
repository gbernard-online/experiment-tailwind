{
  "author": "Ghislain Bernard <ghislain.bernard@gmail.com>",
  "dependencies": {
    "@parcel/core": "2.8",
    "@parcel/transformer-pug": "2.8",
    "@parcel/transformer-sass": "2.8",
    "@types/node": "18.11",
    "@typescript-eslint/eslint-plugin": "5.46",
    "@typescript-eslint/parser": "5.46",
    "by-node-env": "2.0",
    "eslint": "8.29",
    "htmlhint": "1.1",
    "parcel": "2.8",
    "postcss": "8.4",
    "rimraf": "3.0",
    "sharp": "0.31",
    "tailwindcss": "3.2",
    "typescript": "4.9"
  },
  "description": "experiment",
  "license": "MIT",
  "name": "@gbernard/experiment-tailwind",
  "scripts": {
    "build": "by-node-env",
    "build:development": "parcel build --no-optimize --no-source-maps",
    "build:production": "parcel build --no-source-maps",
    "clean": "rimraf .parcel-cache dist node_modules",
    "lint": "eslint --max-warnings=0 src/script.ts",
    "postbuild": "rimraf .parcel-cache",
    "prebuild": "rimraf .parcel-cache dist",
    "prestart": "rimraf .parcel-cache dist",
    "start": "by-node-env",
    "start:development": "parcel serve",
    "start:production": "parcel watch --no-hmr --no-source-maps",
    "version": "echo $npm_package_version"
  },
  "source": "src/index.pug",
  "version": "0.0.1"
}
